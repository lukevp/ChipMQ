<Window x:Class="UI.Debugger"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Title="Debugger" Height="300" Width="300">
    <Grid>
        <TextBlock x:Name="memory" HorizontalAlignment="Left" Margin="10,67,0,0" TextWrapping="Wrap" Text="{Binding Path=CPU.Memory}" VerticalAlignment="Top" Height="193" Width="274"/>

    </Grid>
</Window>
